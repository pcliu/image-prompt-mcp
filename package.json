{
  "name": "image-prompt-mcp",
  "version": "0.1.0",
  "description": "An MCP server for image prompt generation and management",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "serve": "tsx src/index.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:images": "tsx src/tools/__tests__/run-tests.ts",
    "test:images:unit": "tsx src/tools/__tests__/images.test.ts",
    "test:images:integration": "tsx src/tools/__tests__/images.integration.test.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.4",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "winston": "^3.17.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.19.4"
  }
}
